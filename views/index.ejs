<%- include ('partials/header.ejs'); -%>


    <div class="table-wrapper">
        <% if (users ) {%>
            <table class="table table-hovered">
                <thead class="thead-dark">
                    <tr>
                        <th scope="col">ID</th>

                    </tr>
                </thead>
                <tbody>
                    <% users.forEach((user, index) => { %>
                        <tr>
                            <th scope="row"><%= user.id %></th>

                        </tr>
                    <% }) %>
                </tbody>
            </table>
        <% } else { %>
            <p class="text-center">No User found.  </p>
        <% } %>
    </div>
</div>
</body>
</html>
